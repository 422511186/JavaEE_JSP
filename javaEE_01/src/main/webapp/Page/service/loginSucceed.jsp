<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@include file="variable.jsp" %><html><head>    <title>Title</title>    <meta http-equiv="refresh" content="3;url=Page/service/chatRoom.jsp"></head><body><h1><%=request.getParameter("username") + ",   "%> 欢迎回来！</h1><br><h3><%=request.getParameter("password") + ",   "%> 是你的密码,请注意保管！</h3><h3><p id="time">3秒后页面将自动跳转    <p/></h3></body><script>    var second = 3;    var time = document.getElementById("time");    function showTime() {        second--;        time.innerText = second + "秒后页面将自动跳转";    }    setInterval(showTime, 1000);</script></html>